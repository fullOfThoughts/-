.category_middle {
  display: flex;
}
.category_middle_item {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  flex: 1;
  height: 187.5rpx;
}
.category_middle_item image {
  width: 80rpx;
}
.category_middle_item view {
  display: flex;
  align-items: center;
  justify-content: center;
  flex-wrap: wrap;
  margin: 0 auto;
  margin-top: 15rpx;
  border-radius: 50%;
  width: 130rpx;
  height: 130rpx;
  background-color: #91d5ff;
}
.category_middle_item:nth-child(2) view {
  background-color: #b2d235;
}
.category_middle_item:nth-child(3) view {
  background-color: #ef5b9c;
}
.category_middle_item:nth-child(4) view {
  background-color: #f391a9;
}
/* floor */
.floor_item {
  overflow: hidden;
}
.floor_item view:nth-child(1) {
  height: 66rpx;
  line-height: 66rpx;
  padding-left: 20rpx;
  font-weight: 700;
}
.floor_image image {
  width: 32%;
  float: left;
  margin-left: 7rpx;
}
.floor_image image:nth-child(n + 4) {
  margin-top: 3rpx;
}
