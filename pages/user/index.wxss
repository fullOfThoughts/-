.top {
  position: relative;
  height: 49vh;
}
.kalak {
  overflow: hidden;
  width: 100%;
  height: 45vh;
}
.backup {
  height: 45vh;
  background-color: #c4290e;
  display: flex;
  justify-content: center;
  align-items: center;
}
.top_top {
  display: flex;
  flex-direction: column;
  align-items: center;
  position: absolute;
  top: 0;
  left: 0;
  overflow: hidden;
  width: 100%;
  height: 45vh;
}
.top_image {
  z-index: -11;

  filter: blur(5px);
  width: 100%;
  height: 45vh;
}
.top_top_zhong {
  width: 150rpx;
  height: 150rpx;
  border-radius: 50%;
  overflow: hidden;
  margin-top: 105rpx;
  margin-bottom: 90rpx;
}
.top_top_con {
  color: white;
}
.top_top_zhong_image {
  width: 100%;
  height: 100%;
}
.bottom {
  position: absolute;
  width: 92%;
  background-color: white;
  height: 95rpx;
  bottom: 0;
  transform: translateX(-50%);
  left: 50%;
  display: flex;
}
.bottom_item {
  flex: 1;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-around;
}
.bottom_item_top {
  color: red;
}
.footer {
  height: 51vh;
  background-color: #eee;
  padding: 10rpx;
}
.footer_top {
  padding: 7rpx;
  height: 27%;
  background-color: #fff;
  display: flex;
  flex-direction: column;
}
.footer_top_top {
  flex: 1;
  border-bottom: 1px solid #eee;
  padding-left: 5rpx;
}
.footer_top_bottom {
  flex: 2;
  display: flex;
}
.footer_top_bottom_item {
  flex: 1;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-around;
}
.footer_top_bottom_item view:nth-child(1) {
  color: red;
  font-size: 44rpx;
}
.footer_con1 {
  height: 12%;
  margin: 10rpx 0;
  background-color: #fff;
  display: flex;
  align-items: center;
  padding-left: 10rpx;
}
.footer_con2 {
  height: 36%;
  margin-bottom: 10rpx;
  background-color: #fff;
  display: flex;
  flex-direction: column;
}
.footer_con2 view {
  flex: 1;
  display: flex;
  align-items: center;
  padding: 0 10rpx;
}
.footer_con2 view:nth-child(1) {
  display: flex;
  justify-content: space-between;
}
.footer_con2 view:nth-child(-n + 2) {
  border-bottom: 1px solid #ccc;
}
.footer_footer {
  height: 12%;
  background-color: #fff;
  padding: 0 10rpx;
  display: flex;
  align-items: center;
}
